<p id="notice"><%= notice %></p>

<h1>Address Geocodes</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longtitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @address_geocodes.each do |address_geocode| %>
      <tr>
        <td><%= address_geocode.address %></td>
        <td><%= address_geocode.latitude %></td>
        <td><%= address_geocode.longtitude %></td>
        <td><%= link_to 'Show', address_geocode %></td>
        <td><%= link_to 'Edit', edit_address_geocode_path(address_geocode) %></td>
        <td><%= link_to 'Destroy', address_geocode, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Address Geocode', new_address_geocode_path %>
